      aria-hidden="true"
    >
      <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="informationModalTitle">
              Information Declaration
            </h5>
            <button
              type="button"
              class="close btn"
              data-dismiss="modal"
              aria-label="Close"
            >
              <span aria-hidden="true"><i class="fas fa-times"></i></span>
            </button>
          </div>
          <form
            id="formInformation"
            action="/user"
            method="post"
            enctype="multipart/form-data"
          >
            <div class="modal-body">
              <!-- 2 column grid layout with text inputs for the first and last names -->
              <div class="row mb-4">
                <div class="col">
                  <div class="form-outline">
                    <label class="form-label" for="formFullName"
                      >Full name</label
                    >
                    <input
                      name="fullName"
                      type="text"
                      id="formFullName"
                      class="form-control"
                    />
                  </div>
                </div>
                <div class="col">
                  <div class="form-outline">
                    <label class="form-label" for="formDateOfBirth"
                      >Date of Birth</label
                    >
                    <div class="input-group date" id="datetimepicker8">
                      <input
                        name="dateOfBirth"
                        type="text"
                        id="formDateOfBirth"
                        class="form-control"
                      />
                    </div>
                  </div>
                </div>
              </div>

              <!-- nationality input -->
              <div class="row mb-4">
                <div class="col">
                  <div class="form-outline">
                    <label class="form-label" for="formGender">Gender</label>
                    <select
                      name="gender"
                      id="formGender"
                      class="form-control chosen-select"
                    >
                      <option value="" disabled selected>Gender</option>
                      <option value="male">Male</option>
                      <option value="female">Female</option>
                      <option value="other">Other</option>
                    </select>
                  </div>
                </div>
                <div class="col">
                  <div class="form-outline">
                    <label class="form-label" for="formNationality"
                      >Nationality</label
                    >
                    <select
                      name="nationality"
                      id="formNationality"
                      class="form-control chosen-select"
                    >
                      <option value="" disabled selected>
                        Select nationality
                      </option>
                    </select>
                  </div>
                </div>
              </div>

              <div class="row">
                <div class="col">
                  <div class="form-outline">
                    <label class="form-label" for="formFacebookLink"
                      >Facebook</label
                    >
                    <div class="input-group" id="formFacebookLink">
                      <input
                        name="facebookUrl"
                        type="text"
                        id="formFacebook"
                        class="form-control"
                      />
                    </div>
                  </div>
                  <div class="form-outline">
